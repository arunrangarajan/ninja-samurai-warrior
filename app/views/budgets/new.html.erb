<% provide(:title, 'New Budget') %>
<h1>Enter Your Budget!</h1>

<div class="col-md-6 col-md-offset-3">
  <%= form_for @budget, url: budget_path do |f| %>
    <%= render 'shared/error_messages', object: f.object %>
    <%= f.label :income %>
        <%= f.number_field :income, in: 0.0..999999.99, step: 0.01, class: 'form-control' %>
    <%= f.submit "Sumbit", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>